[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Airbnb
ExecStart=/home/ubuntu/Airbnb/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/Airbnb/Airbnb.sock Airbnb.sock:application

[Install]
WantedBy=multi-user.target